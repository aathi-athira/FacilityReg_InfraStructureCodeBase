{
  "formatting": {
    "insertFinalNewline": true,
    "newlineAtEndOfFile": true,
    "indentKind": "Tab",
    "indentSize": 2
  },
  "analyzers": {
    "core": {
      "enabled": true,
      "rules": {
        "no-hardcoded-env-urls": { "level": "warning" },
        "secure-parameter-default": { "level": "warning" }
      }
    }
  }
}
